<?xml version="1.0" encoding="UTF-8"?>
<pnml>
  <net id="net3" type="http://www.pnml.org/version-2009/grammar/ptnet">
    <place id="p0">
      <name><text>p0</text></name>
      <initialMarking><text>1</text></initialMarking>
    </place>
    <place id="p1">
      <name><text>p1</text></name>
    </place>
    <place id="p2">
      <name><text>p2</text></name>
    </place>

    <transition id="t0">
      <name><text>t0</text></name>
    </transition>
    <transition id="t1">
      <name><text>t1</text></name>
    </transition>
    <transition id="t2">
      <name><text>t2</text></name>
    </transition>

    <!-- t0: p0 -> p1 -->
    <arc id="a0" source="p0" target="t0" />
    <arc id="a1" source="t0" target="p1" />

    <!-- t1: p1 -> p0 -->
    <arc id="a2" source="p1" target="t1" />
    <arc id="a3" source="t1" target="p0" />

    <!-- t2: p1 -> p2 -->
    <arc id="a4" source="p1" target="t2" />
    <arc id="a5" source="t2" target="p2" />

    <!--Simple Cycle-->
  </net>
</pnml>
